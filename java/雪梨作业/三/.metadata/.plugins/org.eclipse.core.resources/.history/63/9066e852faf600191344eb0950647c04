package W1024work;

public class Dian {
	private double x;
	private double y;
	public double getX() {
		return x;
	}
	public void setX(double x) {
		this.x = x;
	}
	public double getY() {
		return y;
	}
	public void setY(double y) {
		this.y = y;
	}
	public Dian(){}
	public Dian(double x1,double y1){
		this.x=x1;
		this.y=y1;
	}

}
