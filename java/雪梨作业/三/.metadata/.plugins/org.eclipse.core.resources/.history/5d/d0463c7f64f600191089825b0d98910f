package W1024work;

public class Main {

	public static void main(String[] args) {
		Dian a1=new Dian(0,0);
		Dian a2=new Dian(0,3);
		Dian a3=new Dian(4,0);
		Dian a4=new Dian(0,0);
		
		Xian x1=new Xian(a1,a3);
		Xian x2=new Xian(a3,a4);
		
		Jiao j1=new Jiao(x1,x2);
		System.out.println("三角形的周长："+j1.meter());
	}

}
